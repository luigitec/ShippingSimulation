{
  "name": "luigitec/shipping-simulation",
  "type": "library",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A SortedLinkedList for order prioritization",
  "autoload": {
    "psr-4": {
      "SortedList\\": "src/SortedList/",
      "ShipMonk\\": "src/ShipMonk/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SortedList\\Tests\\": "tests/Unit/",
      "ShipMonk\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.3",
    "squizlabs/php_codesniffer": "^3.6",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpstan/phpstan": "^1.10",
    "shipmonk/dead-code-detector": "^0.5.1"
  },
  "scripts": {
    "test": "phpunit",
    "analyze": "phpstan analyse src --level=8",
    "simulate": "php examples/basic_simulation.php"
  }
}
